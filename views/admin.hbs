                    <div class="pull-left">
                      <a href="#" user_id="{{user.id}}" class="btn btn-default btn-flat user_profiler">Профиль</a>
                    </div>
                    <div class="pull-right">
                      <a href="/logout" class="btn btn-default btn-flat btn_logout">Выход</a>
                    </div>
