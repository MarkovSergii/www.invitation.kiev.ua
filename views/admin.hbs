                      <li {{#IFactive_sub v11 '7'}}  class="active" {{/IFactive_sub}}><a id="smtp_edit"  class="admin_menu_item" href="/admin/menu/settings_smtp"><i class="fa fa-envelope-o"></i>Настройка SMTP</a></li>
                      <li {{#IFactive_sub v11 '8'}}  class="active" {{/IFactive_sub}}><a id="other_settings_edit"  class="admin_menu_item" href="/admin/menu/other_settings"><i class="fa fa-envelope-o"></i>Другие настройки</a></li>
