                <li {{#IFactive v1 '1'}}  class="active" {{/IFactive}}><a id = "admin_dashboard" class="admin_menu_item"  href="/admin/menu/admin_dashboard"><i class="fa fa-dashboard"></i> <span>Состояние сайта</span></a></li>


                <li {{#IFactive v1 '2'}}  class="active" {{/IFactive}}><a id="exhibition_calender" class="admin_menu_item" href="/admin/menu/exhibition_calender"><i class="fa fa-calendar"></i> <span>Выставки</span></a></li>
